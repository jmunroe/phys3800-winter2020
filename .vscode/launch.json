    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "Debug enumsaw",
            "target": "./enumsaw",
            "cwd": "${workspaceRoot}/Ch07-Random-Processes",
            "valuesFormatting": "parseText",
            "arguments": "8 output.txt",
            "preLaunchTask": "build"
        },
    ]
}